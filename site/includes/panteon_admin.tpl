<tr>
    <td><input type="hidden" value="{id}" class="id_panteon"></td>
      
    <td><input type="text" value="{nick_name}" class="panteon_name" oncontextmenu='return false' onpaste="return false" onkeyup="onkey(this)" onkeydown="onkey(this)"></td>
    
    <td><input type="text" value="{user_url}" class="panteon_url" oncontextmenu='return false' onkeydown="onkeyurl(this)"onkeyup="onkeyurl(this)"></td>
      
    <td><input type="text" value="{prestige}" class="prestige" oncontextmenu='return false' onpaste="return false" onkeydown="onkeynumber(this)"onkeyup="onkeynumber(this)" maxlength="7"></td>
      
    <td><input type="text" value="{time_in}" class="time_in" oncontextmenu='return false' onpaste="return false" onkeyup="onkeytime(this)" onkeydown="onkeytime(this)"></td>
     
    <td><input type="text" value="{time_out}" class="time_out" oncontextmenu='return false' onpaste="return false" onkeyup="onkeytime(this)" onkeydown="onkeytime(this)"></td>
      
    <td><input type="text" value="{class}" class="class" oncontextmenu='return false' onpaste="return false" onkeyup="onkey(this)" onkeydown="onkey(this)"></td>
      
    <td><input type="submit" value="{value}"  class="{change}"></td>
       
    <td><input type="{delete}" value="Удалить" class="delete"></td>

</tr>