<tr>
    
    <td><input type="hidden" value="{id}" class="id_user"></td>
        
    <td><input type="text" value="{user_login}" class="user_login" oncontextmenu='return false' onpaste="return false" onkeyup="onkey(this)" onkeydown="onkey(this)"></td>
    
    <td><input type="text" value="{user_password}" class="user_password" oncontextmenu='return false' onpaste="return false" onkeyup="onkey(this)" onkeydown="onkey(this)"></td>
    
    <td><input type="text" value="{nickname}" class="user_nickname" oncontextmenu='return false' onpaste="return false" onkeyup="onkey(this)" onkeydown="onkey(this)"></td>
    
    <td><input type="text" value="{email}" class="email" oncontextmenu='return false' onpaste="return false" onkeyupurl="onkey(this)" onkeydownurl="onkey(this)"></td>
    
    <td><input type="submit" value="{value}"  class="{change}"></td>
    
    <td><input type="{delete}" value="Удалить" class="delete"></td>

</tr>